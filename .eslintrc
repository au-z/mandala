{
	"env": {
		"browser": true,
		"es6": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"root": true,
	"rules": {
		"comma-dangle": ["error", "only-multiline"],
		"keyword-spacing": 0,
		"no-trailing-spaces": 0,
		"require-jsdoc": 0,
		"semi": ["error", "never"],
		"space-before-function-paren": 0,
    "linebreak-style": 0
	}
}